using System;

namespace matrci1
{
    class Program
    {
        static void Main(string[] args)
        {
            Console.WriteLine("Введіть кількість елементів масиву: \t");
            int CountMas = int.Parse(Console.ReadLine());
            int[] mas = new int[CountMas];
            Console.WriteLine("\n Введіть ваш масив:");
            for (int i =0; i <mas.Length;i++)
            {
                
                mas[i] = int.Parse(Console.ReadLine());
            }
Console.WriteLine("Вивести усі елементи номери яких є парними:");
            for (int i = 0; i < mas.Length; i++)
            {
                if (i % 2 == 0)
                    Console.Write(mas[i] + " ");
            }

            Console.ReadLine();

        }
        
        
    }
}







