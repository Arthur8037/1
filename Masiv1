using System;

namespace matrci1
{
    class Program
    {
        static void Main(string[] args)
        {
            Console.WriteLine("Введіть кількість елементів масиву: \t");
            int CountMas = int.Parse(Console.ReadLine());
            int[] mas = new int[CountMas];
            Console.WriteLine("\n Введіть ваш масив:");
            for (int i = 0; i < mas.Length; i++)
            {

                mas[i] = int.Parse(Console.ReadLine());
            }
            int counter=0;

            Console.WriteLine("\n Вивести кількість позитивних елементів масиву:");
            for (int i = 0; i < mas.Length; i++)
            {

                if(mas[i]>0)
                {
                    counter++;
                }    
            }
            Console.WriteLine(counter);
            
            

            Console.ReadLine();

        }


    }
}



Завдання 4



using System;

namespace matrci1
{
    class Program
    {
        static void Main(string[] args)
        {
            Console.WriteLine("Введіть кількість елементів масиву: \t");
            int CountMas = int.Parse(Console.ReadLine());
            int[] mas = new int[CountMas];
            Console.WriteLine("\n Введіть ваш масив:");
            for (int i = 0; i < mas.Length; i++)
            {

                mas[i] = int.Parse(Console.ReadLine());
            }
            int counter=0;

            Console.WriteLine("\n Вивести кількість елементів масиву, більших за попередній:");
            for (int i = 1; i < mas.Length; i++)
            {

                if (mas[i] > mas[i - 1])
                {
                    counter++;
                }    
            }
            Console.WriteLine(counter.ToString());
            
            

            Console.ReadLine();

        }


    }
}




Завдання 5



using System;

namespace matrci1
{
    class Program
    {
        static void Main(string[] args)
        {
            Console.WriteLine("Введіть кількість елементів масиву: \t");
            int CountMas = int.Parse(Console.ReadLine());
            int[] mas = new int[CountMas];
            Console.WriteLine("\n Введіть ваш масив:");
            for (int i = 0; i < mas.Length; i++)
            {

                mas[i] = int.Parse(Console.ReadLine());
            }
            int counter=0;

            Console.WriteLine("\n Вивести слово YES,якщо є пара сусідніх елементів з однаковими знаками. ");
            Console.WriteLine(" Інакше слід вивести слово NO.:");
            for (int i = 0; i < mas.Length - 1; i++)
            {

                if (mas[i] > 0 && mas[i + 1] > 0 || mas[i] < 0 && mas[i + 1] < 0)

                    ++counter;

                

            }
            if (counter >= 2)

                Console.WriteLine("\nYes");
            else Console.WriteLine("\nNo");





            Console.ReadLine();

        }


    }
}




