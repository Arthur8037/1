using System;

namespace matrci1
{
    class Program
    {
        static void Main(string[] args)
        {
            Console.WriteLine("Введіть кількість елементів масиву: \t");
            int CountMas = int.Parse(Console.ReadLine());
            int[] mas = new int[CountMas];
            Console.WriteLine("\n Введіть ваш масив:");
            for (int i = 0; i < mas.Length; i++)
            {

                mas[i] = int.Parse(Console.ReadLine());
            }
            Console.WriteLine("\n Ваш масив :");
            for (int i = 0; i < mas.Length; i++)
            {

                Console.WriteLine(mas[i]);
            }

            Console.WriteLine("\n Вивести елементи масиву у зворотному порядку: ");
            for (int i=mas.Length-1;i>=0;i--)
            {
                Console.WriteLine(mas[i]);
            }

            




            Console.ReadLine();

        }


    }
}




Завдання 8




using System;

namespace matrci1
{
    class Program
    {
        static void Main(string[] args)
        {
            Console.WriteLine("Введіть кількість елементів масиву: \t");
            int CountMas = int.Parse(Console.ReadLine());
            int[] mas = new int[CountMas];
            Console.WriteLine("\n Введіть ваш масив:");
            for (int i = 0; i < mas.Length; i++)
            {

                mas[i] = int.Parse(Console.ReadLine());
            }
            Console.WriteLine("\n Ваш масив :");
            for (int i = 0; i < mas.Length; i++)
            {

                Console.WriteLine(mas[i]);
            }

            Console.WriteLine("\n Вивести масив, отриманий після перестановки елементів: ");
            var rslt = exchange(mas);
           
            Console.WriteLine (string.Join(" ", rslt));
            Console.ReadKey();

        }
        public static int[] exchange(int[] array)
        {
            int[] _temp_array = new int[array.Length];
            array.CopyTo(_temp_array, 0);
            for (int n = 0; n < array.Length; n += 2)
            {
                if ((n == array.Length - 1) && (array.Length % 2 == 1)) break;
                int a = _temp_array[n];
                int b = _temp_array[n + 1];
                _temp_array[n] = b;
                _temp_array[n + 1] = a;
            }
            return _temp_array;
        }

    }
}



Завдання 9




using System;

namespace matrci1
{
    class Program
    {
        static void Main(string[] args)
        {
            Console.WriteLine("Введіть кількість елементів масиву: \t");
            int CountMas = int.Parse(Console.ReadLine());
            int[] mas = new int[CountMas];
            Console.WriteLine("\n Введіть ваш масив:");
            for (int i = 0; i < mas.Length; i++)
            {

                mas[i] = int.Parse(Console.ReadLine());
            }
            Console.WriteLine("\n Ваш масив :");
            for (int i = 0; i < mas.Length; i++)
            {

                Console.WriteLine(mas[i]);
            }

            Console.WriteLine("\n Вивести масив, отриманий після зсуву елементів.: ");
            int tmp = mas[mas.Length - 1];
            for (int i = mas.Length - 2; i >=0; i--)
            {
                mas[i+1] = mas[i];
                
                
            }
            mas[0] = tmp;
            for (int i=0;i<mas.Length;i++)
            {
                Console.WriteLine(mas[i]);
            }
            Console.ReadKey();

        }
        

    }
}




Завдання 10




using System;

namespace matrci1
{
    class Program
    {
        static void Main(string[] args)
        {
            Console.WriteLine("Введіть кількість елементів масиву: \t");
            int CountMas = int.Parse(Console.ReadLine());
            int[] mas = new int[CountMas];
            Console.WriteLine("\n Введіть ваш масив:");
            for (int i = 0; i < mas.Length; i++)
            {

                mas[i] = int.Parse(Console.ReadLine());
            }
            Console.WriteLine("\n Ваш масив :");
            for (int i = 0; i < mas.Length; i++)
            {

                Console.WriteLine(mas[i]);
            }

            Console.WriteLine("\n Знайти найбільше число у масиві: ");
            int max = mas[0];
            for(int i=1;i<mas.Length;i++)
            {
                if(mas[i]>max)
                {
                    max = mas[i];
                }
            }
            Console.WriteLine(max);
            Console.ReadKey();

        }
        

    }
}






Завдання 11




#include <iostream>
using namespace std;
int main()
{
    int n;
    cin >> n;
    int* a = new int[n];
    for (int i = 0; i < n; i++) { cin >> a[i];
    }
    int diffNum = 0;
    for (int i = 0; i < n; i++)
    {
        bool newNum = true;
        for (int j = 0; j < i && a[i-1] != a[j]; j++)
        {
            if (a[j] == a[i])
            {
                newNum = false;
            }
        }
        if (newNum == true)
        {
            diffNum++;
        }
    }
    cout << diffNum;
    
    return 0;
}




Завдання 13



#include <iostream>
using namespace std;
int main(){
        int n,a,b,c,d;
        cin>>n>>a>>b>>c>>d;
        int x[1001],y[1001];
        for(int i=1;i<=n;i++){
            x[i]=i;y[i]=x[i];
        }
        for(int i=0;i<=b-a;i++)
              {
                  x[a+i]=y[b-i];
              }
        for(int i=1;i<=n;i++){
           y[i]=x[i];}
 
        for(int i=0;i<=d-c;i++)
              {
                  x[c+i]=y[d-i];
              }
 
        for(int i=1;i<=n;i++){
            cout<<x[i]<<' ';
        }
return 0;
}





Завдання 14


#include <iostream>
#include <cmath>
using namespace std;
 
int main()
{
    unsigned long long arr[1000000]; int a, b, c = 0, d, i = 0; int k;
    cin >> b;
 
    for (a = 0; a < b; a++)
    {
        cin >> arr[a];
    }
 
    cin >> k;
 
    if (k < 0)
    {
       for (i = 0; i < abs(k); i++)
        {
          for (a = 0; a < b; a++)
            {
 
               if (a == 0)
                { d = arr[a];
                }
                arr[a] = arr[a + 1];
 
                if (a == b - 1)
                { arr[a] = d;
                }
            }
        }
    }
 
    else
    {
        for (i = 0; i < k; i++)
        {
            for (a = b - 1; a >= 0; a--)
            {
 
                if (a == b - 1)
                { d = arr[a];
                }
                arr[a] = arr[a - 1];
 
                if (a == 0)
                {  arr[a] = d;
                }
            }
        }
    }
 
    for (a = 0; a < b; a++)
    {
        cout << arr[a] << " ";
    }
 
    cout << endl;
    system("pause");
    return 0;
}




Завдання 16




a = []
n = int(input())
for i in range (n):
    a.append([0]*n)
for i in range (n):
    for j in range(n):
        if i == j:
            a[i][j] = 1
        elif i > j:
            a[i][j] = 2
        else:
            a[i][j] = 0
for i in range(n):
    for j in range(n-1,-1,-1):
        print(a[i][j], end = ' ')
    print()
    
    
    


