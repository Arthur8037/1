Завдання 23



#include <iostream> 
 
using namespace std;
 
int main ()
{ 
    int N, M;
    cout << "Enter N (strings): ";
    cin >> N;
    cout << "Enter M (columns): ";
    cin >> M;
    cout << endl;
 
    N++; M++;
 
    int ** Arr = new int *[N]; 
    for (int i = 0; i < N; i++)
        Arr[i] = new int[M];
 
    int nA=0, mA=0;
 
    if (N > M) nA=N-M;
    else if (M > N) mA=M-N; 
   
    for(int i=0; i<N*M; ++i) 
        Arr[i/(N+mA)][i%(M+nA)]=(i/(N+mA))*(i%(M+nA)); 
    
    cout << "Multiplication table " << N-1 << " x " << M-1 << endl << endl;
    
    for(int i=1;i<N;++i) 
    {
        for(int j=1;j<M;++j) 
        {
            cout.width(4);
            cout<<Arr[i][j];
        }
        cout<<endl;
    }
 
    for (int i = 0; i < N; i++) 
        delete []Arr[i];
 
    system ("PAUSE");
    return 0;
}




Завдання 24




#include <bits/stdc++.h>
using namespace std;
int main()
{
    int n, m;
    cin >> n >> m;
    int a[n][m];
    for(int i = 0; i < n; ++i){
        a[i][0] = 1;
    }
    for(int i = 0; i < m; ++i){
        a[0][i] = 1;
    }
    for(int i = 1; i < n; ++i)
        for(int j = 1; j < m; ++j)
            a[i][j] = a[i - 1][j] + a[i][j - 1];
    for(int i = 0; i < n; ++i){
        cout << " ";
        for(int j = 0; j < m; ++j){
            cout << a[i][j] << " ";
        }
        cout << endl;
    }
    return 0;
}




Завдання 25



package com.company;
import  java.lang.String;
import java.util.Scanner;
class Main {
    public static void main(String[] args) {
        System.out.print("Введите n: ");
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        int[][] b = new int[2 * n + 1][2 * n + 1];
        int f = 0;
        b[n][n] = f;
        for (int g = 1; g <= n; g++) {
 
            for (int i = 0; i <= g; i++) {
                f = f + 1;
                b[n - g][n - i] = f;
            }
            for (int i = 0; i <= g; i++) {
                f = f + 1;
                b[n + i][n - g] = f;
            }
            for (int i = 0; i <= g; i++) {
                f = f + 1;
                b[n + g][n + i] = f;
            }
            if ((n > 1)&&(g<2)) {
                for (int i = 0; i <= g+1; i++) {
                    f = f + 1;
                    b[n - i][n + g] = f;
                }
            } else {
                for (int i = 0; i <= g; i++) {
                    f = f + 1;
                    b[n - i][n + g] = f;
                }
            }
 
        }
        for (int i = 0; i < n * 2 + 1; i++) {
            for (int j = 0; j < n * 2 + 1; j++)
                System.out.print(b[i][j] + "  ");
            System.out.println();
        }
    }
}
